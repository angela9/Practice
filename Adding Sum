class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target){
        vector<int> ans;
        typedef vector<int>::iterator it;
        for(it i = nums.begin(); i != nums.end(); i++) {
            for (it j = i+1; j!=nums.end(); j++) {
                if(nums.at(*i)+nums.at(*j)==target) {
                    ans.push_back(*i);
                    ans.push_back(*j);
                }
            }
        }
        return ans;
    }
};
